@using GamersAddict.Models
@{
    ViewBag.Title = "Shop - Xbox One";
}

<div id="shopDiv">
    @Html.Partial("Shared/_NavPartial")
     <img class="pure-img" style="width: 100%;" src="~/Images/Shop/XboxOne.png" alt="Xbox One">


    <div class="page-content-body pure-g">
        <div class="pure-u-md-6-24">
            
        </div>
    </div>
</div>

<link rel="stylesheet" href="~/Content/owl-carousel/owl.carousel.css">
<link rel="stylesheet" href="~/Content/owl-carousel/owl.theme.css">
@section Scripts {
    <script src="~/Content/owl-carousel/owl.carousel.min.js"></script>
    <script>
        $(document).ready(function () {

            $("#owl-slide").owlCarousel({
                navigation: false, // Show next and prev buttons
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true
            });

        });
    </script>
}